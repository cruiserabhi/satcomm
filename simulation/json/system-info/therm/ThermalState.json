/*     "maxCoolingLevel": 1,         cooling device maximum level, Valid value: 0 to INT32 max                                 */
/*     "currentCoolingLevel": 0      cooling device current level, Valid value: 0 to INT32 max                                 */
/* }                                                                                                                           */

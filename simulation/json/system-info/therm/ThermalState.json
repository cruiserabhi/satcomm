/* ##################################################### EXAMPLE ############################################################# */
/* {                                                                                                                           */
/*    "desc": "mdmss-0",              thermal zone name,                                          Valid value: 0 to 32 char    */
/*    "id": 24,                       thermal zone id,                                            Valid value: 0 to INT32 max  */
/*    "temp": 36400,                  thermal zone current temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*    "passiveTemp": -274000,         thermal zone passive temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*        "tripPoints": [             no of trip points associated with thermal zone                                           */
/*            {                                                                                                                */
/*                "id": 0,            trip point id,                                              Valid value: 0 to INT32 max  */
/*                "temp": 135000,     trip point threshold temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*                "hyst": 5000,       trip point hysteresis in millidegree Celsius,               Valid value: 0 to INT32 max  */
/*                "type": "passive"   trip point type defined under telux::therm::TripType                                     */
/*            },                                                                                                               */
/*            {                                                                                                                */
/*                "id": 1,            trip point id,                                              Valid value: 0 to INT32 max  */
/*                "temp": 117000,     trip point threshold temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*                "hyst": 3000,       trip point hysteresis in millidegree Celsius,               Valid value: 0 to INT32 max  */
/*                "type": "passive"   trip point type defined under telux::therm::TripType                                     */
/*            },                                                                                                               */
/*            {                                                                                                                */
/*                "id": 2,            trip point id,                                              Valid value: 0 to INT32 max  */
/*                "temp": 120000,     trip point threshold temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*                "hyst": 3000,       trip point hysteresis in millidegree Celsius,               Valid value: 0 to INT32 max  */
/*                "type": "passive"   trip point type defined under telux::therm::TripType                                     */
/*            },                                                                                                               */
/*            {                                                                                                                */
/*                "id": 3,            trip point id,                                              Valid value: 0 to INT32 max  */
/*                "temp": 125000,     trip point threshold temperature in millidegree Celsius,    Valid value: 0 to INT32 max  */
/*                "hyst": 5000,       trip point hysteresis in millidegree Celsius,               Valid value: 0 to INT32 max  */
/*                "type": "passive"   trip point type defined under telux::therm::TripType                                     */
/*            }                                                                                                                */
/*        ],                                                                                                                   */
/*        "boundCoolingDevices": [    no of cooling devices associated with each trip points                                   */
/*            {                                                                                                                */
/*                "id": 6,            cooling device id,                                          Valid value: 0 to INT32 max  */
/*                "tripPoints": [     trip point associated with cooling device                                                */
/*                    {                                                                                                        */
/*                        "id": 1,    trip point id,                                              Valid value: 0 to INT32 max  */
/*                        "level": 11,set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max  */
/*                        "clr": 0    clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max  */
/*                    },                                                                                                       */
/*                    {                                                                                                        */
/*                        "id": 2,     trip point id,                                              Valid value: 0 to INT32 max */
/*                        "level": 255,set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max */
/*                        "clr": 11    clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max */
/*                    }                                                                                                        */
/*                ]                                                                                                            */
/*            },                                                                                                               */
/*            {                                                                                                                */
/*                "id": 8,            cooling device id,                                          Valid value: 0 to INT32 max  */
/*                "tripPoints": [     trip point associated with cooling device                                                */
/*                    {                                                                                                        */
/*                        "id": 1,    trip point id,                                              Valid value: 0 to INT32 max  */
/*                        "level": 11,set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max  */
/*                        "clr": 0    clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max  */
/*                    },                                                                                                       */
/*                    {                                                                                                        */
/*                        "id": 2,    trip point id,                                              Valid value: 0 to INT32 max  */
/*                        "level": 255,set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max */
/*                        "clr": 11   clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max  */
/*                    }                                                                                                        */
/*                ]                                                                                                            */
/*            },                                                                                                               */
/*            {                                                                                                                */
/*                "id": 10,           cooling device id,                                          Valid value: 0 to INT32 max  */
/*                "tripPoints": [     trip point associated with cooling device                                                */
/*                    {                                                                                                        */
/*                        "id": 1,    trip point id,                                              Valid value: 0 to INT32 max  */
/*                        "level": 11,set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max  */
/*                        "clr": 0    clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max  */
/*                    }                                                                                                        */
/*                ]                                                                                                            */
/*          },                                                                                                                 */
/*          {                                                                                                                  */
/*              "id": 28,            cooling device id,                                          Valid value: 0 to INT32 max   */
/*              "tripPoints": [      trip point associated with cooling device                                                 */
/*                  {                                                                                                          */
/*                      "id": 2,     trip point id,                                              Valid value: 0 to INT32 max   */
/*                      "level": 3,  set level to be set when temp CROSS_OVER to trip id 1,      Valid value: 0 to INT32 max   */
/*                      "clr": 0     clear level to be set when temp CROSSED_UNDER to trip id 1, Valid value: 0 to INT32 max   */
/*                  }                                                                                                          */
/*              ]                                                                                                              */
/*          }                                                                                                                  */
/*      ]                                                                                                                      */
/* In the above example,                                                                                                       */
/* Each ThermalZone consist of multiple tripPoints and bound cooling devices.                                                  */
/* One or more bound cooling devices can be attached with multiple trip points.                                                */
/* The boundCoolingDevice id 6, level is set to 11 if the temperature crosses over the trip id 1 threshold (117000).           */
/* Next, the boundCoolingDevice id 6, level is set to 255 if the temperature crosses over the trip id 2 threshold (120000).    */
/* Next, if the temperature crosses under the trip id 1 threshold (117000) immediately, the boundCoolingDevice id 6 level      */
/* will be set to 11 instead of 0.                                                                                             */
/* To reach bound cooling device level to 0, it requires another cycle of decreament in temprature since all the bound         */
/* cooling devices are step down in nature.                                                                                    */
/*                                                                                                                             */
/*############################################# Cooling device example ####################################################### */
